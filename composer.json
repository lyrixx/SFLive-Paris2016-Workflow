{
    "name": "lyrixx/sf-live-paris2016-workflow",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.16.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "symfony/apache-pack": "^1.0.1",
        "symfony/asset": "^7.4.0",
        "symfony/console": "^7.4.0",
        "symfony/dotenv": "^7.4.0",
        "symfony/expression-language": "^7.4",
        "symfony/flex": "^2.8.2",
        "symfony/form": "^7.4.0",
        "symfony/framework-bundle": "^7.4.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/process": "^7.4.0",
        "symfony/property-access": "^7.4.0",
        "symfony/property-info": "^7.4.0",
        "symfony/runtime": "^7.4.0",
        "symfony/security-bundle": "^7.4.0",
        "symfony/serializer": "^7.4.0",
        "symfony/translation": "^7.4.0",
        "symfony/twig-bundle": "^7.4",
        "symfony/validator": "^7.4.0",
        "symfony/web-link": "^7.4",
        "symfony/workflow": "^7.4",
        "symfony/yaml": "^7.4.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^7.4.0",
        "symfony/css-selector": "^7.4",
        "symfony/debug-bundle": "^7.4",
        "symfony/maker-bundle": "^1.64.0",
        "symfony/phpunit-bridge": "^7.4",
        "symfony/stopwatch": "^7.4.0",
        "symfony/var-dumper": "^7.4.0",
        "symfony/web-profiler-bundle": "^7.4.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php84": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4",
            "docker": false
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
